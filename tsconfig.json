{
  "compilerOptions": {
    "strict": true,
    "outDir": "bin",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "sourceMap": false,
    "esModuleInterop": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "pretty": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["vite/client", "vitest/globals", "@testing-library/jest-dom"]
    
  },
  "include": ["lambdas/**/*.ts", "www/**/*.tsx"]
}
